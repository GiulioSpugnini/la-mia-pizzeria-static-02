<body>
    @{

        string name = "Giulio";
        string surname = "Spugnini";
        string text = "nel mezzo del cammin di nostra vita";
        List<int> numbers;

        Func<dynamic, IHtmlContent> SommaNumeri(int n) 
        {
            int sum = 0;

            for(int i=1; i <= n; i++)
            {
                sum += i;
            }
            return
            @<p>@sum</p>
        ;
        }Func<dynamic, IHtmlContent> ContaA() 
        {
            int cont = 0;
            for(int i=0; i < text.Length; i++)
            {
              if(text[i].ToString().ToLower()=="a")  cont += 1;
            }
            return
            @<span>@cont</span>
        ;
        }Func<dynamic, IHtmlContent> MakeListOfNumbers() 
        {

            for(int i=1; i<20; i++)
            {
                Random rand = new Random();
                numbers[i] =rand.Next();

            }
            return
        @<span>@numbers</span> ;
        }
    }

    <h2>
        @name @surname
    </h2>
    @SommaNumeri(21)(1)
    @ContaA()(1)
    @MakeListOfNumbers()

